{
  "family": "backoffice-server-dev",
  "executionRoleArn": "ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "backoffice-server-dev",
      "image": "193142563486.dkr.ecr.ap-northeast-2.amazonaws.com/backoffice-server-dev:latest",
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "command": ["python", "manage.py", "runserver", "0:8000"],
      "environment": [
        {
          "name": "API_TYPE",
          "value": "office"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "backoffice-server-dev",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "office",
          "awslogs-create-group": "true"
        }
      }
    },
    {
      "name": "backoffice-server-dev-company",
      "image": "193142563486.dkr.ecr.ap-northeast-2.amazonaws.com/backoffice-server-dev:latest",
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "command": ["python", "manage.py", "runserver", "0:8000"],
      "environment": [
        {
          "name": "API_TYPE",
          "value": "company"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "backoffice-server-dev",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "company",
          "awslogs-create-group": "true"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "1024"
}
